{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "strict": false,
    "alwaysStrict": false,
    "strictNullChecks": false,
    "noUncheckedIndexedAccess": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowUnreachableCode": true,
    "suppressExcessPropertyErrors": true,
    "noFallthroughCasesInSwitch": false,
    "target": "es5",
    "outDir": "out",
    "declaration": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "isolatedModules": true,
    "incremental": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@public/*": ["public/*"],
      "@public": ["public"],
      "@definitions": ["src/definitions"],
      "@interfaces/*": ["src/interfaces/*"],
      "@interfaces": ["src/interfaces"],
      "@styles": ["src/styles"],
      "@components/icons/*": ["src/ui/icons/*"],
      "@components/icons": ["src/ui/icons"],
      "@services": ["src/services"],
      "@graphqlTypes/*": ["__generated__/*"],
      "@graphqlTypes": ["__generated__"],
      "@test/*": ["test/*"],
      "@test": ["test"],
      "@services/*": ["src/services/*"],
      "@sections/*": ["src/ui/sections/*"],
      "@components/*": ["src/ui/*"],
      "@definitions/*": ["src/definitions/*"],
      "@ui/*": ["src/ui/*"],
      "@types/*": ["src/interfaces/*"],
      "@utils/*": ["srcutils/*"],
      "@styles/*": ["src/ui/styles/*"],
      "@icons/*": ["src/ui/icons/*"],
      "@i18n": ["./i18n"]
    }
  },
  "include": ["next-env.d.ts", "src/**/*.ts", "src/**/*.tsx", "pages/**/*.ts", "pages/**/*.tsx"],
  "exclude": ["node_modules", "__generated__/globalTypes.ts", "test/*", "cypress/*"]
}
