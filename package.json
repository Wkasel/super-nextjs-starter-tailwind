{
    "name": "whim-web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
        "build:analyze": "ANALYZE=true npm run build",
        "apollo:download-schema": "apollo client:download-schema schema.graphql",
        "apollo:generate-types": "rm -rf __generated__ && apollo client:codegen --target typescript --outputFlat",
        "apollo:sync": "npm run apollo:download-schema && npm run apollo:generate-types",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "test": "jest -c test/jest.config.js",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run --config video=false",
        "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
    },
    "dependencies": {
        "@apollo/client": "^3.3.7",
        "@headlessui/react": "^1.4.1",
        "@heroicons/react": "^1.0.5",
        "@next/bundle-analyzer": "^10.0.5",
        "classnames": "^2.3.1",
        "clsx": "^1.1.1",
        "date-fns": "^2.25.0",
        "graphql": "^15.6.0",
        "graphql-scalars": "^1.10.1",
        "graphql-tag": "^2.12.5",
        "next": "12.0.7",
        "next-compose-plugins": "^2.2.1",
        "next-i18next": "^7.0.1",
        "next-images": "^1.8.1",
        "next-page-transitions": "^1.0.0-beta.2",
        "next-seo": "^4.26.0",
        "next-themes": "^0.0.15",
        "nprogress": "^0.2.0",
        "prettier": "^2.2.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-dropzone": "^11.4.2",
        "react-hook-form": "^7.19.2",
        "react-mailchimp-subscribe": "^2.1.3",
        "react-query": "^3.5.16",
        "react-select": "^5.2.1",
        "react-social-icons": "^5.6.1",
        "react-use": "^15.3.8",
        "styled-components": "^5.2.1",
        "styled-jsx-plugin-postcss": "^4.0.1",
        "styled-system": "^5.1.5",
        "swr": "^0.4.0",
        "tailwindcss": "^2.0.2",
        "twin.macro": "^2.7.0",
        "use-modal-hook": "^1.0.19",
        "uuid": "^8.3.2",
        "valtio": "^1.2.3"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@next/bundle-analyzer": "^11.1.2",
        "@storybook/addon-actions": "^6.1.14",
        "@storybook/addon-essentials": "^6.1.14",
        "@storybook/addon-knobs": "^6.1.14",
        "@storybook/addon-links": "^6.1.14",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/preset-scss": "^1.0.3",
        "@storybook/react": "^6.1.14",
        "@svgr/cli": "^5.5.0",
        "@types/enzyme": "^3.10.11",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^27.4.0",
        "@types/micro-cors": "^0.1.2",
        "@types/node": "^14.14.13",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
        "apollo": "^2.32.1",
        "autoprefixer": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-styled-components": "^1.12.0",
        "cypress": "^6.2.1",
        "dotenv": "^8.2.0",
        "enzyme": "^3.11.0",
        "eslint": "^7.15.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-airbnb-typescript": "^14.0.0",
        "eslint-config-next": "^11.1.2",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.2.0",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-unused-imports": "^1.1.4",
        "husky": "^4.3.7",
        "identity-obj-proxy": "^3.0.0",
        "isomorphic-unfetch": "^3.1.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "next-plugin-custom-babel-config": "^1.0.5",
        "nock": "^13.0.6",
        "postcss": "^8.2.4",
        "start-server-and-test": "^1.11.0",
        "storybook-addon-styled-component-theme": "^1.3.0",
        "style-loader": "^2.0.0",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "npm run lint -- --quiet --fix"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
